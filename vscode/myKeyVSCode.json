// Place your key bindings in this file to overwrite the defaults
[
  {
    "key": "ctrl+shift+h",
    "command": "deleteLeft",
    "when": "textInputFocus && !editorReadonly"
  },
  {
    "key": "ctrl+h",
    "command": "-deleteLeft",
    "when": "textInputFocus && !editorReadonly"
  },
  {
    "key": "ctrl+shift+alt+cmd+l",
    "command": "cursorRight",
    "when": "textInputFocus"
  },
  {
    "key": "ctrl+f",
    "command": "-cursorRight",
    "when": "textInputFocus"
  },
  {
    "key": "ctrl+shift+alt+cmd+h",
    "command": "cursorLeft",
    "when": "textInputFocus"
  },
  {
    "key": "ctrl+b",
    "command": "-cursorLeft",
    "when": "textInputFocus"
  },
  {
    "key": "ctrl+alt+tab",
    "command": "text-tables.gotoNextCell",
    "when": "tableMode"
  },
  {
    "key": "tab",
    "command": "-text-tables.gotoNextCell",
    "when": "tableMode"
  },
  {
    "key": "ctrl+shift+alt+tab",
    "command": "text-tables.gotoPreviousCell",
    "when": "tableMode"
  },
  {
    "key": "shift+tab",
    "command": "-text-tables.gotoPreviousCell",
    "when": "tableMode"
  },
  {
    "key": "ctrl+n",
    "command": "-cursorDown",
    "when": "textInputFocus"
  },
  {
    "key": "ctrl+p",
    "command": "-cursorUp",
    "when": "textInputFocus"
  },
  {
    "key": "ctrl+alt+o",
    "command": "editor.action.openLink"
  },
  {
    "key": "ctrl+alt+o",
    "command": "seito-openfile.openFileFromText",
    "when": "editorTextFocus"
  },
  {
    "key": "alt+p",
    "command": "-seito-openfile.openFileFromText",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+k v",
    "command": "-markdown.showPreviewToSide",
    "when": "editorLangId == 'markdown'"
  },
  {
    "key": "cmd+k v",
    "command": "-markdown.extension.togglePreviewToSide",
    "when": "!terminalFocus"
  },
  {
    "key": "cmd+u",
    "command": "cursorPageUp",
    "when": "textInputFocus"
  },
  {
    "key": "pageup",
    "command": "cursorPageUp",
    "when": "textInputFocus"
  },
  {
    "key": "cmd+d",
    "command": "cursorPageDown",
    "when": "textInputFocus"
  },
  {
    "key": "pagedown",
    "command": "cursorPageDown",
    "when": "textInputFocus"
  },
  {
    "key": "alt+cmd+o",
    "command": "outline.focus"
  },
  {
    "key": "ctrl+shift+z",
    "command": "editor.action.toggleWordWrap"
  },
  {
    "key": "alt+z",
    "command": "-editor.action.toggleWordWrap"
  },
  {
    "key": "cmd+k cmd+f",
    "command": "editor.action.formatDocument",
    "when": "editorHasDocumentFormattingProvider && editorHasDocumentFormattingProvider && editorTextFocus && !editorReadonly"
  },
  {
    "key": "shift+alt+f",
    "command": "-editor.action.formatDocument",
    "when": "editorHasDocumentFormattingProvider && editorHasDocumentFormattingProvider && editorTextFocus && !editorReadonly"
  },
  {
    "key": "cmd+k `",
    "command": "markdown.extension.editing.toggleStrikethrough",
    "when": "editorTextFocus && !editorReadonly && editorLangId == 'markdown'"
  },
  {
    "key": "alt+s",
    "command": "-markdown.extension.editing.toggleStrikethrough",
    "when": "false"
  },
  {
    "key": "ctrl+k",
    "command": "-deleteAllRight",
    "when": "textInputFocus && !editorReadonly"
  },
  {
    "key": "ctrl+shift+j",
    "command": "editor.action.joinLines",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+j",
    "command": "-editor.action.joinLines",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+d",
    "command": "-deleteRight",
    "when": "textInputFocus"
  },
  {
    "key": "cmd+k cmd+g",
    "command": "git.commitAll"
  },
  {
    "key": "cmd+k cmd+p",
    "command": "git.push"
  },
  {
    "key": "cmd+k cmd+left",
    "command": "workbench.action.focusLeftGroup"
  },
  {
    "key": "cmd+k cmd+left",
    "command": "-workbench.action.focusLeftGroup"
  },
  {
    "key": "ctrl+cmd+t",
    "command": "workbench.action.terminal.focus"
  },
  {
    "key": "cmd+k cmd+r",
    "command": "editor.action.goToReferences",
    "when": "editorHasReferenceProvider && editorTextFocus && !inReferenceSearchEditor && !isInEmbeddedEditor"
  },
  {
    "key": "shift+f12",
    "command": "-editor.action.goToReferences",
    "when": "editorHasReferenceProvider && editorTextFocus && !inReferenceSearchEditor && !isInEmbeddedEditor"
  },
  {
    "key": "cmd+p",
    "command": "workbench.action.quickOpenNavigateNextInFilePicker",
    "when": "inFilesPicker && inQuickOpen"
  },
  {
    "key": "cmd+p",
    "command": "-workbench.action.quickOpenNavigateNextInFilePicker",
    "when": "inFilesPicker && inQuickOpen"
  },
  {
    "key": "ctrl+l",
    "command": "expandLineSelection",
    "when": "textInputFocus"
  },
  {
    "key": "cmd+l",
    "command": "-expandLineSelection",
    "when": "textInputFocus"
  },
  {
    "key": "ctrl+shift+alt+cmd+t",
    "command": "terminal.focus"
  },
  {
    "key": "alt+b",
    "command": "workbench.action.focusSideBar"
  },
  {
    "key": "cmd+0",
    "command": "-workbench.action.focusSideBar"
  },
  {
    "key": "shift+alt+b",
    "command": "workbench.files.action.showActiveFileInExplorer"
  },
  {
    "key": "ctrl+shift+alt+cmd+r",
    "command": "workbench.action.openRecent"
  },
  {
    "key": "ctrl+r",
    "command": "-workbench.action.openRecent"
  },
  {
    "key": "cmd+b",
    "command": "-workbench.action.toggleSidebarVisibility"
  },
  {
    "key": "ctrl+shift+alt+cmd+j",
    "command": "cursorDown",
    "when": "textInputFocus"
  },
  {
    "key": "ctrl+shift+alt+cmd+k",
    "command": "cursorUp",
    "when": "textInputFocus"
  },
  {
    "key": "ctrl+shift+alt+cmd+v",
    "command": "pasteURL.PasteURL"
  },
  {
    "key": "ctrl+alt+p",
    "command": "-pasteURL.PasteURL"
  },
  {
    "key": "cmd+z",
    "command": "undo"
  },
  {
    "key": "shift+cmd+z",
    "command": "redo"
  },
  {
    "key": "ctrl+shift+alt+cmd+b",
    "command": "workbench.action.toggleSidebarVisibility"
  },
  {
    "key": "alt+c",
    "command": "cursorMove",
    "args": {
      "to": "wrappedLineColumnCenter",
      "select": false  // default is false
    }
  }
]
